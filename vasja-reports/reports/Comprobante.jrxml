	<queryString language="SQL">
		<![CDATA[(SELECT
     operacion.`CHEQUENUMERO` AS operacion_CHEQUE_NUMERO,
     operacion.`DESCRIPCION` AS operacion_DESCRIPCION,
     operacion.`DNI` AS operacion_DNI,
     operacion.`FECHA` AS operacion_FECHA,
